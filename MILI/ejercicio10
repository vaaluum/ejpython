def calcular_calif_final():
   
    n1 = float(input("Ingrese la primer calificacion: "))
    n2= float(input("Ingrese la segunda calificación: "))
    n3 = float(input("Ingrese la tercera calificación: "))
    
    calif_final = (n1 * 0.30) + (n2 * 0.20) + (n3 * 0.50)
    
   
    print("La calificación final es:", calif_final)

calcular_calif_final()
